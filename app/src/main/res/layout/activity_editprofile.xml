    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginBottom="5dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp">

            <ImageView
                android:layout_height="30dp"
                android:id="@+id/back_btn"
                android:adjustViewBounds="true"
                android:padding="2dp"
                android:src="@drawable/arrow_left1"
                android:tint="#948067"
                android:layout_width="25dp"
                android:layout_marginTop="5dp" />

            <ImageView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:textStyle="bold"
                android:textColor="@android:color/white"
                android:src="@drawable/logo_icon"
                android:padding="7dp"
                android:layout_marginTop="5dp" />

            <TextView
                android:layout_width="60dp"
                android:layout_height="25dp"
                android:id="@+id/logout"
                android:text="LogOut"
                android:adjustViewBounds="true"
                android:gravity="center_vertical|center_horizontal"
                android:textColor="#948067"
                android:textSize="17sp"
                android:layout_marginRight="2dp"
                android:layout_marginTop="5dp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CAAD73"></LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/item_image"
            android:src="@drawable/ic_profile"
            android:layout_height="96dp"
            android:layout_width="96dp" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:text="00"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/user_no_posts"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView68"
                        android:text="Posts"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp" />

                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_weight="1">

                    <TextView
                        android:text="000"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/followers"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp" />

                    <TextView
                        android:text="Followers"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView70"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="10dp">

                    <TextView
                        android:text="000"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/following"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp" />

                    <TextView
                        android:text="Following"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView72"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="5dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:text="Edit Profile"
                        android:gravity="center_vertical|center_horizontal"
                        android:background="@drawable/edit_profile_btn_bg"
                        android:textColor="#948067"
                        android:id="@+id/edit_btn"
                        android:layout_height="30dp"
                        android:layout_width="100dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="10dp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:id="@+id/edit_image_btn"
                        android:src="@drawable/ic_chats"
                        android:adjustViewBounds="true"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="5dp"
                        android:visibility="visible" />
                </LinearLayout>

            </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:id="@+id/date"
                    android:layout_gravity="right"
                    android:gravity="center_horizontal">

                    <TextView
                        android:text="Expire Date:"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="10dp" />

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginTop="7dp"
                        android:id="@+id/exp_date" />

                </LinearLayout>



        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:background="#CAAD73"
        android:layout_marginTop="5dp"
        android:layout_height="2dp"
        android:elevation="1dp"></LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:layout_height="wrap_content"
                android:id="@+id/images_post"
                android:src="@drawable/ic_points"
                android:layout_width="match_parent"
                android:layout_gravity="center_vertical|center_horizontal"
                android:adjustViewBounds="false"
                android:layout_weight="1" />

            <ImageView
                android:layout_height="wrap_content"
                android:id="@+id/posts_list"
                android:src="@drawable/ic_gall"
                android:layout_width="match_parent"
                android:adjustViewBounds="false"
                android:layout_weight="1" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:background="#CAAD73"
        android:layout_height="2dp"></LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="5dp"
        android:id="@+id/frame_one">

    </FrameLayout>

</LinearLayout>